            },
            "BulletEmailAddressAppPassword": {
                "Type": "String",
                "NoEcho": "true"
            },
            "RecEmail": {
                "Type": "String",
                "NoEcho": "true"
            },
            "RunningMapPath": {
                "Type": "String",
                "NoEcho": "true"
            },
            "GoogleApiKey": {
                "Type": "String",
                "NoEcho": "true"
            },
            "GoogleMapsApiPath": {
                "Type": "String",
                "NoEcho": "true"
            }
          }
        }
      }
    }
